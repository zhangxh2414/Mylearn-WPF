        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfOpenfile"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="400">
    <DockPanel Width="Auto" Height="Auto" LastChildFill="True" >
        <StackPanel Width="Auto" Height="269" Background="LightGray" VerticalAlignment="Bottom">
          
            <!--标签-->
            <Label Content="IFC路径：" Margin="0,10,0,0"></Label>
            <TextBox Name="txtbox" Text=" "></TextBox>
            <!--按钮-->
            <Button Content="Open IFC File" Margin="0,2,0,0" Name="Btnopen" Click="Btnopen_Click_1"></Button>
        </StackPanel>
        
    </DockPanel>
        
 
</Window>
